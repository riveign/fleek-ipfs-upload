{
    "name": "@genql/runtime",
    "_": "[bump]",
    "version": "2.2.5",
    "description": "Genql runtime client",
    "repository": {
        "url": "https://github.com/remorses/genql"
    },
    "homepage": "https://genql.now.sh",
    "main": "dist/index.js",
    "sideEffects": false,
    "module": "esm/index.js",
    "types": "dist/index.d.ts",
    "mocha": {
        "require": "tests/init.js",
        "spec": "tests/**.ts",
        "timeout": 9999999999
    },
    "files": ["/dist/*", "/esm/*", "src"],
    "scripts": {
        "start": "tsdx watch",
        "build": "rm -rf dist esm *.tsbuildinfo && cp ./package.json src/version.json && tsc && tsc -m ESNext --outDir esm",
        "typesTests": "tsc -b src/client/__tests__/tsconfig.json",
        "test": "yarn typesTests && jest",
        "compile": "sucrase -q ./src -d ./dist --transforms typescript,imports",
        "dev": "yarn compile && node dist"
    },
    "keywords": [],
    "author": "Tommaso De Rossi, morse <beats.by.morse@gmail.com>",
    "license": "ISC",
    "devDependencies": {
        "@sucrase/jest-plugin": "^2.0.0",
        "@types/common-tags": "^1.8.0",
        "@types/jest": "^23.3.13",
        "@types/listr": "^0.13.0",
        "@types/lodash": "^4.14.161",
        "@types/mkdirp": "^0.5.2",
        "@types/node": "^12.0.7",
        "@types/rimraf": "^2.0.2",
        "common-tags": "^1.8.0",
        "dotenv": "^8.2.0",
        "jest": ">=22 <24",
        "sucrase": "^3.12.1",
        "ts-jest": "^23.10.5"
    },
    "dependencies": {
        "@types/qs": "^6.9.0",
        "@types/ws": "^6.0.1",
        "graphql-query-batcher": "^1.0.1",
        "isomorphic-unfetch": "^3.0.0",
        "lodash": "^4.17.20",
        "subscriptions-transport-ws": "^0.9.16",
        "tslib": "^2.0.0",
        "utility-types": "^3.10.0",
        "ws": "^6.1.4",
        "zen-observable-ts": "^0.8.21"
    },
    "jest": {
        "transform": {
            ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
        },
        "roots": ["<rootDir>/src"],
        "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
        "modulePaths": ["<rootDir>"],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "testEnvironment": "node",
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!**/node_modules/**",
            "!src/_old/**",
            "!**/*.case.ts",
            "!src/testHelpers/**/*.ts"
        ],
        "coverageReporters": ["text"]
    }
}
